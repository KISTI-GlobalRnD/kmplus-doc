{
    "name": "kmplus-doc",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "npm run build && npm run serve",
        "build": "docusaurus build",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "deploy": "docusaurus deploy",
        "dev": "docusaurus start",
        "pdf:single": "node scripts/generate-pdf.js",
        "pdf": "npm run build && start-process powershell -ArgumentList \"-Command\",\"npm run serve\" && timeout /t 5 && node scripts/generate-full-pdf.js",
        "pdf:local": "node scripts/generate-full-pdf.js"
    },
    "dependencies": {
        "@docusaurus/core": "3.9.2",
        "@docusaurus/preset-classic": "3.9.2",
        "@easyops-cn/docusaurus-search-local": "^0.52.2",
        "@mdx-js/react": "^3.0.0",
        "clsx": "^2.0.0",
        "prism-react-renderer": "^2.3.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "3.9.2",
        "@docusaurus/tsconfig": "3.9.2",
        "@docusaurus/types": "3.9.2",
        "docusaurus-pdf": "^1.2.0",
        "docusaurus-prince-pdf": "^1.2.1",
        "pdf-lib": "^1.17.1",
        "puppeteer": "^24.32.0",
        "typescript": "~5.6.2"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
